{
  "name": "ssg-starter",
  "version": "1.0.0",
  "private": true,
  "description": "ssg-starter",
  "author": "Nico Ismaili",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "dev": "dotenvx run --env-file=.env.local -- gatsby develop -o",
    "dev:clean": "yarn clean && yarn dev",
    "dev:v": "yarn dev --verbose",
    "start": "gatsby develop",
    "build": "gatsby build",
    "build:local": "dotenvx run --env-file=.env.local -- gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ci": "yarn test:coverage && yarn test:e2e",
    "test:e2e": "yarn playwright test",
    "test:coverage": "vitest run --coverage",
    "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:types",
    "lint:eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:prettier": "prettier --check .",
    "lint:types": "tsc --noEmit",
    "postinstall": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^0.14.1",
    "autoprefixer": "^10.4.17",
    "gatsby": "^5.13.2",
    "gatsby-plugin-google-gtag": "^5.13.1",
    "gatsby-plugin-manifest": "^5.13.1",
    "gatsby-plugin-postcss": "^6.13.1",
    "gatsby-plugin-react-i18next": "^3.0.1",
    "gatsby-plugin-robots-txt": "^1.8.0",
    "gatsby-plugin-sitemap": "^6.13.1",
    "gatsby-source-filesystem": "^5.13.1",
    "graphql": "^16.8.1",
    "i18next": "^23.8.2",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.33",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.1",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@parcel/watcher": "^2.4.0",
    "@playwright/test": "^1.41.2",
    "@testing-library/jest-dom": "^6.4.1",
    "@testing-library/react": "^14.2.1",
    "@types/node": "^20.11.5",
    "@types/node-fetch": "^2.6.11",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vitest/coverage-v8": "^1.2.2",
    "c8": "^9.1.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^9.0.10",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.1",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  }
}
