{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["dom", "esnext"],
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "@types": ["./src/types/index.ts"],
      "@helper/*": ["./src/helper/*"],
      "@components/*": ["./src/components/*"]
    },
    "esModuleInterop": true,

    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "./src/**/*",
    "./gatsby-browser.ts",
    "./gatsby-config.ts",
    "./plugins/**/*"
  ]
}
